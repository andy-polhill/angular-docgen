    "@types/node": "^11.11.3",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "semantic-release": "^15.13.3",
